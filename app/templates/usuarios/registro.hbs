{{outlet}}
<h1>Registro de usuario</h1>

<h2>Datos personales</h2>
<div class='column'>
    {{input type='text' value=nombreUsuario placeholder='Nombre De Usuario'}}
    <br> 
    {{input type='password' value=contraseña placeholder='Contraseña'}}
    <br> 
    {{input type='password' value=confirmacionContraseña placeholder='Confirmación de la contraseña'}}
    <br>
    {{input type='text' value=nombre placeholder='Nombre'}}
    <br>
    Fecha de Nacimiento{{input type='date' value=fechaNacimiento}}
    </div>

<div class='column'>
    Género{{input type='radio' value=genero}}Masculino{{input type='radio' value=genero}}Femenino
    <br>
    {{input type='text' value=ocupacion placeholder='Ocupación'}}
    <br> 
    {{textarea type='text' value=biografia placeholder='Biografía'}}
    <br>
    <select>
        <option disabled selected> País </option>
    </select>
    <select>
        <option disabled selected> Provincia </option>
    </select>
    <select>
        <option disabled selected> Ciudad </option>
    </select>
    <br>
    <button>Subir foto</button> <button>Seleccionar archivo</button>
</div>

<div class='column'>
    <img src='icon.jpg'>
</div>

<div class='column'>
    <h2>Criterio de emparejamiento</h2>     
    Género Preferido{{input type='radio' value=genero}}Masculino{{input type='radio' value=genero}}Femenino
    <select>
        <option disabled selected> No considerar ubicación </option>
    </select>
    {{input type='range' value=rangoEdadPreferido min=18 max=100}}
</div>